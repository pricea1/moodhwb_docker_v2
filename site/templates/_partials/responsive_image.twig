{% if image.type == "image" and image.imageFile and image.imageFile.first() %}
<img 	srcset="	{{ image.imageFile.first().getUrl('medium') }} 768w,
		{{ image.imageFile.first().getUrl('large') }} 1024w"
		sizes="(min-width: 49em) 50vw, 100vw"
		alt="{{image.imageAlt}}"
		src="{{ image.imageFile.first().getUrl('medium') }}"
>


{% endif %}